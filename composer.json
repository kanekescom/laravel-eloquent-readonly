{
    "name": "kanekescom/laravel-eloquent-readonly",
    "type": "library",
    "description": "Laravel Eloquent Readonly Package",
    "license": "MIT",
    "authors": [
        {
            "name": "Achmad Hadi Kurnia",
            "email": "imachmadhadikurnia@gmail.com",
            "homepage": "https://achmadhadikurnia.com"
        }
    ],
    "homepage": "https://github.com/kanekescom/laravel-eloquent-readonly",
    "keywords": [
        "Laravel",
        "EloquentReadonly"
    ],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Kanekescom\\LaravelEloquentReadonly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kanekescom\\LaravelEloquentReadonly\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kanekescom\\LaravelEloquentReadonly\\EloquentReadonlyServiceProvider"
            ],
            "aliases": {
                "EloquentReadonly": "Kanekescom\\LaravelEloquentReadonly\\Facades\\EloquentReadonly"
            }
        }
    }
}
